							<m:cn>0</m:cn>
						</m:apply>
					</m:math></para>
			</solution>
		</exercise><exercise id="element-353"><problem id="idm933664">
				<para id="element-260"><m:math>
						<m:apply><m:eq/>
							<m:apply>
								<m:fn><m:ci>f</m:ci></m:fn>
								<m:ci>x</m:ci>
							</m:apply>
							<m:apply><m:divide/>
								<m:apply><m:times/>
									<m:apply><m:times/>
										<m:cn>3</m:cn>
										<m:ci>x</m:ci>
									</m:apply>
									<m:apply><m:minus/>
										<m:ci>x</m:ci>
										<m:cn>1</m:cn>
									</m:apply>
								</m:apply>
								<m:apply><m:minus/>
									<m:apply><m:minus/>
										<m:apply><m:times/>
											<m:cn>2</m:cn>
											<m:apply><m:power/>
												<m:ci>x</m:ci>
												<m:cn>2</m:cn>
											</m:apply>
										</m:apply>
										<m:apply><m:times/>
											<m:cn>5</m:cn>
											<m:ci>x</m:ci>
										</m:apply>
									</m:apply>
									<m:cn>3</m:cn>
								</m:apply>
							</m:apply>
						</m:apply>
					</m:math></para>
			</problem>
			<solution id="idm2809888">
				<para id="element-609"><m:math>
						<m:apply><m:eq/>
							<m:ci>y</m:ci>
							<m:apply><m:divide/><m:cn>3</m:cn><m:cn>2</m:cn></m:apply>
						</m:apply>
					</m:math>
				</para>
			</solution>
		</exercise><exercise id="element-684"><problem id="idp712928">
				<para id="element-24">
					<m:math>
						<m:apply><m:eq/>
							<m:apply>
								<m:fn><m:ci>q</m:ci></m:fn>
								<m:ci>x</m:ci>
							</m:apply>
							<m:apply><m:divide/>
								<m:apply><m:times/>
									<m:apply><m:minus/>
										<m:ci>x</m:ci>
									</m:apply>
									<m:apply><m:minus/>
