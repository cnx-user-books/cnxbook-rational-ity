										<m:ci>x</m:ci>
									</m:apply>
								</m:apply>
								<m:apply><m:minus/>
									<m:apply><m:plus/>
										<m:apply><m:times/>
											<m:cn>3</m:cn>
											<m:apply><m:power/>
												<m:ci>x</m:ci>
												<m:cn>2</m:cn>
											</m:apply>
										</m:apply>
										<m:apply><m:times/>
											<m:cn>5</m:cn>
											<m:ci>x</m:ci>
										</m:apply>
									</m:apply>
									<m:cn>2</m:cn>
								</m:apply>
							</m:apply>
						</m:apply>
					</m:math>
				</para>
			</problem>
			<solution id="idp3175360">
				<para id="element-209"><m:math>
						<m:apply><m:eq/>
							<m:ci>y</m:ci>
							<m:apply><m:divide/><m:cn>1</m:cn><m:cn>3</m:cn></m:apply>
						</m:apply>
					</m:math></para>
			</solution>
		</exercise><exercise id="element-570"><problem id="idm8900992">
				<para id="element-422">
					<m:math>
						<m:apply><m:eq/>
							<m:apply>
								<m:fn><m:ci>r</m:ci></m:fn>
