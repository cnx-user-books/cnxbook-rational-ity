<m:interval closure="open-closed">
<m:apply><m:minus/><m:cn>5</m:cn></m:apply>
<m:cn>2</m:cn>
</m:interval>
</m:math></para>
	</solution>
</exercise><exercise id="element-126"><problem id="idp1427856">
