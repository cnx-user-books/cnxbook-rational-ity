<m:apply><m:union/>
	<m:list>
		<m:apply><m:minus/><m:cn>2</m:cn></m:apply>
	</m:list>
	<m:list>
		<m:cn>0</m:cn>
	</m:list>
	<m:list>
		<m:cn>2</m:cn>
	</m:list>
	<m:list>
		<m:cn>3</m:cn>
	</m:list>
</m:apply>
</m:math></para>
<para id="element-995">Range: <m:math>
<m:apply><m:union/>
	<m:list>
		<m:apply><m:minus/><m:cn>1</m:cn></m:apply>
	</m:list>
	<m:list>
		<m:cn>1</m:cn>
	</m:list>
	<m:list>
		<m:cn>2</m:cn>
	</m:list>
	<m:list>
		<m:cn>3</m:cn>
	</m:list>
</m:apply>
</m:math></para>
	</solution>
